function goTo(targetX, targetZ)
    local currentX, currentY, currentZ = gps.locate(5)
    if not currentX then
        print("GPS not available. Make sure you're in range of a GPS satellite network.")
        return
    else
        print("Starting position: X=" .. currentX .. ", Y=" .. currentY .. ", Z=" .. currentZ)
    end

    -- Move to target X
    local moveCount = 0
    while currentX ~= targetX do
        if currentX < targetX then
            while not turtle.forward() do turtle.dig() end
            moveCount = moveCount + 1
            if moveCount % 2 == 0 then
                currentX, _, _ = gps.locate(5)
                print("Moved 2 blocks forward, current X: " .. currentX)
            end
        elseif currentX > targetX then
            turtle.turnLeft()
            turtle.turnLeft()  -- Now facing west
            while not turtle.forward() do turtle.dig() end
            moveCount = moveCount + 1
            if moveCount % 2 == 0 then
                currentX, _, _ = gps.locate(5)
                print("Moved 2 blocks backward, current X: " .. currentX)
            end
            turtle.turnLeft()
            turtle.turnLeft()  -- Reorient to east
        end
    end

    -- Adjust orientation to south for Z movement
    turtle.turnRight()  -- Now facing south
    print("Adjusted to face south for Z movement.")

    moveCount = 0
    while currentZ ~= targetZ do
        while not turtle.forward() do turtle.dig() end
        moveCount = moveCount + 1
        if moveCount % 2 == 0 then
            _, _, currentZ = gps.locate(5)
            print("Moved 2 blocks forward, current Z: " .. currentZ)
        end
    end

    -- Adjust orientation to face north
    turtle.turnLeft()
    print("Final adjustment to face north.")
end

-- Call the goTo function with your coordinates
goTo(-337, -16)
